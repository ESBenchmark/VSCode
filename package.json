{
	"name": "esbench-vscode",
	"description": "Run benchmark by clicking on the codelens.",
	"version": "0.1.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Kaciras/esbench-vscode"
	},
	"displayName": "ESBench Runner",
	"engines": {
		"vscode": "^1.85.0"
	},
	"main": "dist/extension.js",
	"categories": [
		"Other"
	],
	"scripts": {
		"package": "tsc && vsce package --no-dependencies",
		"watch": "tsc --watch",
		"publish": "vsce publish --no-dependencies"
	},
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:javascript",
		"onLanguage:typescriptreact",
		"onLanguage:javascriptreact"
	],
	"devDependencies": {
		"@kaciras/eslint-config-core": "^2.6.3",
		"@kaciras/eslint-config-typescript": "^2.6.3",
		"@types/node": "^20.11.7",
		"@types/vscode": "^1.85.0",
		"@vscode/vsce": "^2.22.0",
		"eslint": "^8.56.0",
		"typescript": "^5.3.3"
	}
}
