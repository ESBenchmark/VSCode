{
	"name": "esbench-vscode",
	"description": "Run benchmark by clicking on the codelens.",
	"version": "0.1.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Kaciras/esbench-vscode"
	},
	"displayName": "ESBench Runner",
	"icon": "logo.png",
	"engines": {
		"vscode": "^1.85.0"
	},
	"main": "dist/extension.js",
	"categories": [
		"Other"
	],
	"scripts": {
		"lint": "eslint --fix",
		"package": "tsc && vsce package --no-dependencies",
		"watch": "tsc --watch",
		"publish": "vsce publish --no-dependencies"
	},
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:javascript",
		"onLanguage:typescriptreact",
		"onLanguage:javascriptreact"
	],
	"devDependencies": {
		"@kaciras/eslint-config-core": "^3.0.1",
		"@kaciras/eslint-config-typescript": "^3.0.1",
		"@types/node": "^20.12.7",
		"@types/vscode": "^1.88.0",
		"@vscode/vsce": "^2.26.0",
		"eslint": "^9.1.1",
		"typescript": "^5.4.5"
	}
}
